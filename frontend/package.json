{
  "name": "I0-frontend",
  "version": "1.0.0",
  "description": "I0- Frontend monorepo project",
  "private": true,
  "workspaces": [
    "app/*",
    "shared"
  ],
  "scripts": {
    "dev": "concurrently \"yarn workspace @I0/client-portal dev\" \"yarn workspace @I0/service-portal dev\" \"yarn workspace @I0/admin-portal dev\" \"yarn workspace @I0/talent-portal dev\"",
    "dev:client": "yarn workspace @I0/client-portal dev",
    "dev:service": "yarn workspace @I0/service-portal dev",
    "dev:admin": "yarn workspace @I0/admin-portal dev",
    "dev:talent": "yarn workspace @I0/talent-portal dev",
    "build": "yarn workspace @I0/client-portal build && yarn workspace @I0/service-portal build && yarn workspace @I0/admin-portal build && yarn workspace @I0/talent-portal build",
    "build:client": "yarn workspace @I0/client-portal build",
    "build:service": "yarn workspace @I0/service-portal build",
    "build:admin": "yarn workspace @I0/admin-portal build",
    "build:talent": "yarn workspace @I0/talent-portal build",
    "preview": "yarn workspace @I0/client-portal preview && yarn workspace @I0/service-portal preview && yarn workspace @I0/admin-portal preview && yarn workspace @I0/talent-portal preview",
    "preview:client": "yarn workspace @I0/client-portal preview",
    "preview:service": "yarn workspace @I0/service-portal preview",
    "preview:admin": "yarn workspace @I0/admin-portal preview",
    "preview:talent": "yarn workspace @I0/talent-portal preview",
    "test": "vitest",
    "test:coverage": "vitest run --config ./configs/vitest.coverage.config.ts",
    "test:bdd": "cucumber-js --config cucumber.config.js",
    "test:bdd:admin-service-type": "cucumber-js --config cucumber.config.js --profile admin-service-type",
    "test:bdd:admin-entity": "cucumber-js --config cucumber.config.js --profile admin-entity",
    "test:bdd:admin-client": "cucumber-js --config cucumber.config.js --profile admin-client",
    "test:bdd:admin-reports": "cucumber-js --config cucumber.config.js --profile admin-reports",
    "test:bdd:pretty": "CUCUMBER_PROGRESS_FORMAT=pretty cucumber-js --config cucumber.config.js",
    "test:bdd:debug": "DEBUG_MODE=true HEADLESS=false cucumber-js --config cucumber.config.js --profile admin-service-type --tags '@debug'",
    "test:bdd:pwdebug": "PWDEBUG=1 HEADLESS=false cucumber-js --config cucumber.config.js",
    "test:bdd:pwdebug:service-type": "PWDEBUG=1 HEADLESS=false cucumber-js --config cucumber.config.js --profile admin-service-type",
    "test:bdd:pwdebug:entity": "PWDEBUG=1 HEADLESS=false cucumber-js --config cucumber.config.js --profile admin-entity",
    "test:bdd:pwdebug:client": "PWDEBUG=1 HEADLESS=false cucumber-js --config cucumber.config.js --profile admin-client",
    "test:bdd:pwdebug:reports": "PWDEBUG=1 HEADLESS=false cucumber-js --config cucumber.config.js --profile admin-reports",
    "test:bdd:trace": "ENABLE_TRACE=true cucumber-js --config cucumber.config.js --profile admin-service-type",
    "test:bdd:video": "ENABLE_VIDEO=true cucumber-js --config cucumber.config.js --profile admin-service-type",
    "test:bdd:full": "ENABLE_TRACE=true ENABLE_VIDEO=true cucumber-js --config cucumber.config.js --profile admin-service-type",
    "test:bdd:report": "cd tests/e2e && node support/allure-reporter.js",
    "test:bdd:allure:open": "npx allure open tests/e2e/reports/allure-report",
    "test:bdd:allure:serve": "npx allure serve tests/e2e/reports/allure-results",
    "test:bdd:allure:generate": "cd tests/e2e && node support/allure-reporter.js",
    "test:bdd:run-and-report": "yarn test:bdd:full && yarn test:bdd:allure:generate",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:client": "eslint app/client-portal --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:service": "eslint app/service-portal --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:admin": "eslint app/admin-portal --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:talent": "eslint app/talent-portal --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:shared": "eslint shared --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf app/*/dist shared/dist"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@cucumber/messages": "^29.0.1",
    "@cucumber/pretty-formatter": "^2.3.0",
    "@playwright/test": "^1.55.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.5.2",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/core": "^13.9.0",
    "allure-commandline": "^2.34.1",
    "allure-cucumberjs": "^3.4.1",
    "code-inspector-plugin": "^1.2.10",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sass-embedded": "1.77.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "dayjs": "^1.11.18",
    "element-plus": "^2.11.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0"
  }
}
