{
  "name": "@I0/shared",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./api": {
      "import": "./src/api/index.ts",
      "types": "./src/api/index.ts"
    },
    "./components": {
      "import": "./src/components/index.ts",
      "types": "./src/components/index.ts"
    },
    "./components/*": {
      "import": "./src/components/*",
      "types": "./src/components/*"
    },
    "./features": {
      "import": "./src/features/index.ts",
      "types": "./src/features/index.ts"
    },
    "./features/*": {
      "import": "./src/features/*",
      "types": "./src/features/*"
    },
    "./composables": {
      "import": "./src/composables/index.ts",
      "types": "./src/composables/index.ts"
    },
    "./utils/*": {
      "import": "./src/utils/*.ts",
      "types": "./src/utils/*.ts"
    },
    "./stores": {
      "import": "./src/stores/index.ts",
      "types": "./src/stores/index.ts"
    },
    "./stores/*": {
      "import": "./src/stores/*.ts",
      "types": "./src/stores/*.ts"
    },
    "./types": {
      "import": "./src/types/index.ts",
      "types": "./src/types/index.ts"
    },
    "./styles": {
      "import": "./src/styles/index.scss",
      "types": "./src/styles/index.scss"
    },
    "./layouts": {
      "import": "./src/layouts/index.ts",
      "types": "./src/layouts/index.ts"
    },
    "./layouts/*": {
      "import": "./src/layouts/*",
      "types": "./src/layouts/*"
    },
    "./config": {
      "import": "./src/config/index.ts",
      "types": "./src/config/index.ts"
    },
    "./config/*": {
      "import": "./src/config/*",
      "types": "./src/config/*"
    }
  },
  "scripts": {
    "build": "vue-tsc && vite build",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "peerDependencies": {
    "pinia": "^2.1.0",
    "vue": "^3.3.0",
    "vue-i18n": "^9.2.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^27.0.0",
    "sass-embedded": "1.77.5",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vitest": "^3.2.4",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.25"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.12.2",
    "echarts": "^6.0.0",
    "nprogress": "^0.2.0",
    "vue-element-plus-x": "^1.3.7"
  }
}